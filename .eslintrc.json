{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "plugin:react/recommended",
        "standard",
        "prettier",
        "airbnb"

    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "prettier"
    ],
    "rules": {
        "no-unused-vars": "off",
        "camelcase": "off",
        "react/prop-types": "off",
        "react/display-name": "off",
        "react/jsx-key": "off",
        "prettier/prettier": ["error"
        ]
    }
}
